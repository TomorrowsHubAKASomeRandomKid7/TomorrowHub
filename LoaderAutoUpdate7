LocalPlayerSettings:AddButton({
	Name = "Refresh Character",
	Callback = function()
		char = plr.Character
		local char = plr.Character
	end
})
LocalPlayerSettings:AddDropdown({
	Name = "Health Type",
	Default = "x1",
	Options = {"x1", "x2", "x3", "x4", "x5", "inf"},
	Callback = function(val)
		if val == "x1" then plr.Character.Humanoid.MaxHealth = "100" plr.Character.Humanoid.Health = "100" end
		if val == "x2" then plr.Character.Humanoid.MaxHealth = "200" plr.Character.Humanoid.Health = "200" end
		if val == "x3" then plr.Character.Humanoid.MaxHealth = "300" plr.Character.Humanoid.Health = "300" end
		if val == "x4" then plr.Character.Humanoid.MaxHealth = "400" plr.Character.Humanoid.Health = "400" end
		if val == "x5" then plr.Character.Humanoid.MaxHealth = "500" plr.Character.Humanoid.Health = "500" end
		if val == "x3" then plr.Character.Humanoid.MaxHealth = math.huge plr.Character.Humanoid.Health = math.huge end
	end
})
LocalPlayerSettings:AddDropdown({
	Name = "WalkSpeed",
	Default = "x1",
	Options = {"x1", "x2", "x3", "x4", "x5", "9B"},
	Callback = function(val)
		if val == "x1" then plr.Character.Humanoid.WalkSpeed = "16" end
		if val == "x2" then plr.Character.Humanoid.WalkSpeed = "32" end
		if val == "x3" then plr.Character.Humanoid.WalkSpeed = "48" end
		if val == "x4" then plr.Character.Humanoid.WalkSpeed = "64" end
		if val == "x5" then plr.Character.Humanoid.WalkSpeed = "80" end
		if val == "9B" then plr.Character.Humanoid.WalkSpeed = "9e9" end
	end
})
LocalPlayerSettings:AddDropdown({
	Name = "JumpPower",
	Default = "x1",
	Options = {"x1", "x2", "x3", "x4", "x5", "9B"},
	Callback = function(val)
		plr.Character.Humanoid.UseJumpPower = true
		if val == "x1" then plr.Character.Humanoid.JumpPower = "50" end
		if val == "x2" then plr.Character.Humanoid.JumpPower = "100" end
		if val == "x3" then plr.Character.Humanoid.JumpPower = "150" end
		if val == "x4" then plr.Character.Humanoid.JumpPower = "200" end
		if val == "x5" then plr.Character.Humanoid.JumpPower = "250" end
		if val == "9B" then plr.Character.Humanoid.JumpPower = "9e9" end
	end
})
