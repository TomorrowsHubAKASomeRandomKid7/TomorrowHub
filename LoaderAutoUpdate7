NwekoSimwulator:AddButton({
	Name = "Crash Server",
	Callback = function()
		while wait() do
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Crashing Server! Made By Tomorrow / tmbmoordenaarsalt! Crashing Server! Made By Tomorrow / tmbmoordenaarsalt! Crashing Server! Made By Tomorrow / tmbmoordenaarsalt!", "All")
            game:GetService("ReplicatedStorage").GivePuroScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GivePurpProtoScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GiveHunterScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GiveNicoScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GiveCandyScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GiveFrostScriptEvent:FireServer()
            game:GetService("ReplicatedStorage").GiveWilsonScriptEvent:FireServer()
        end
	end
})
NwekoSimwulator:AddButton({
	Name = "Neko God/Error Neko",
	Callback = function()
		game.ReplicatedStorage.GivePuroScriptEvent:FireServer()
		game.ReplicatedStorage.GivePurpProtoScriptEvent:FireServer()
		game.ReplicatedStorage.GiveHunterScriptEvent:FireServer()
		game.ReplicatedStorage.GiveNicoScriptEvent:FireServer()
		game.ReplicatedStorage.GiveCandyScriptEvent:FireServer()
		game.ReplicatedStorage.GiveFrostScriptEvent:FireServer()
		game.ReplicatedStorage.GiveWilsonScriptEvent:FireServer()
	end
})
